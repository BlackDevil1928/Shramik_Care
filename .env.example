# Kerala Migrant Health System - Environment Variables
# Copy this file to .env.local and fill in your actual values

# Application Settings
NEXT_PUBLIC_APP_NAME="Kerala Migrant Health System"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXT_PUBLIC_BASE_URL="http://localhost:3000"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Vapi.ai Configuration
VAPI_API_KEY=your_vapi_api_key

# Vapi Assistant IDs for different languages
VAPI_ASSISTANT_EN=your_english_assistant_id
VAPI_ASSISTANT_HI=your_hindi_assistant_id  
VAPI_ASSISTANT_BN=your_bengali_assistant_id
VAPI_ASSISTANT_OR=your_odia_assistant_id
VAPI_ASSISTANT_TA=your_tamil_assistant_id
VAPI_ASSISTANT_NE=your_nepali_assistant_id
VAPI_ASSISTANT_ML=your_malayalam_assistant_id

# SMS Provider Configuration (for MHI confirmations)
SMS_PROVIDER_API_KEY=your_sms_provider_api_key
SMS_PROVIDER_SENDER_ID="KERALA-HEALTH"

# Security Keys
JWT_SECRET=your_jwt_secret_key_for_admin_auth
ENCRYPTION_KEY=your_encryption_key_for_sensitive_data

# Health System Configuration  
HEALTH_EMERGENCY_NUMBER=108
HEALTH_HELPLINE_NUMBER=104
OUTBREAK_THRESHOLD=5

# Deployment Configuration (Production only)
NEXT_PUBLIC_VERCEL_URL=your_production_domain
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_tracking_id

# Development Settings
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1